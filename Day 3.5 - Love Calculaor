# Lowerclass everything
lower_1 = name1.lower()
lower_2 = name2.lower()

# Count the number of every letter of "true" in both names

count_t = lower_1.count("t") + lower_2.count("t")
count_r = lower_1.count("r") + lower_2.count("r")
count_u = lower_1.count("u") + lower_2.count("u")
count_e = lower_1.count("e") + lower_2.count("e")

# Add together
count_true = count_t + count_r + count_u + count_e
# Convert interger to a string
count_true_str = str(count_true)

# Count the number of every letter of "true" in both names
count_l = lower_1.count("l") + lower_2.count("l")
count_o = lower_1.count("o") + lower_2.count("o")
count_v = lower_1.count("v") + lower_2.count("v")
count_e = lower_1.count("e") + lower_2.count("e")

# Add together
count_love = count_l + count_r + count_u + count_e

# Convert interger to a string
count_love_str = str(count_love)

score = count_true_str + count_love_str
score_int = int(score)


if score_int < 10 or score_int > 90:
  print(f"Your score is {score_int}, you go like coke and mentos.")
